#include <bits/stdc++.h>
using namespace std;
#define pb push_back
vector<int>adj[100];
int vis[100];
int level[100];
int parent[100];
void bfs(int src)
{
    queue<int> q;
    q.push(src);
    level[src]=0;
    parent[src]=0;
    while(!q.empty())
    {
        int u=q.front();
        for(auto it = adj[u].begin(); it!= adj[u].end(); it++){
            int v = *it;
            if(!vis[v]){
                level[v]=level[u]+1;
                parent[v]=u;
                vis[v]=1;
                q.push(v);
            }
        }
        q.pop();
    }

}

int main()
{
    int node, egde;
    cin>>node>>egde;
    for(int i=0;i<egde; i++){
        int a,b;
        cin>>a>>b;
        adj[a].pb(b);
        adj[b].pb(a);
    }
   bfs(1);
   cout<<parent[2]<<endl;
   cout<<level[6]<<endl;
}
